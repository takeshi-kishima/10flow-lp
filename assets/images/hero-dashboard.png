  <rect x="300" y="170" width="260" height="50" fill="#ffffff" stroke="#e2e8f0" stroke-width="1" rx="8"/>
  <text x="310" y="195" font-family="Arial, sans-serif" font-size="14" fill="#64748b">顧客満足度</text>
  <text x="310" y="215" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#3b82f6">95%</text>
  
  <!-- Action buttons -->
  <rect x="40" y="240" width="140" height="40" fill="#f97316" rx="20"/>
  <text x="110" y="265" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">分析開始</text>
  
  <rect x="200" y="240" width="140" height="40" fill="#10b981" rx="20"/>
  <text x="270" y="265" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">レポート生成</text>
</svg>
