<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <rect width="600" height="400" fill="#f8fafc"/>
  <rect x="40" y="40" width="520" height="40" fill="#3b82f6" rx="4"/>
  <text x="50" y="65" font-family="Arial, sans-serif" font-size="20" fill="white">10FLOW Dashboard</text>
  
  <!-- Chart area -->
  <rect x="40" y="100" width="240" height="100" fill="#ffffff" stroke="#e2e8f0" stroke-width="1" rx="8"/>
  <text x="50" y="125" font-family="Arial, sans-serif" font-size="14" fill="#64748b">売上推移</text>
  <polyline points="60,180 100,160 140,140 180,120 220,100" stroke="#3b82f6" stroke-width="3" fill="none"/>
  
  <!-- Stats cards -->
  <rect x="300" y="100" width="260" height="50" fill="#ffffff" stroke="#e2e8f0" stroke-width="1" rx="8"/>
