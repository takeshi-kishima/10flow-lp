<svg width="300" height="600" viewBox="0 0 300 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Phone frame -->
  <rect width="300" height="600" fill="#1f2937" rx="25"/>
  <rect x="10" y="10" width="280" height="580" fill="#ffffff" rx="20"/>
  
  <!-- Status bar -->
  <rect x="20" y="20" width="260" height="30" fill="#f8fafc"/>
  <circle cx="40" cy="35" r="3" fill="#10b981"/>
  <text x="50" y="40" font-family="Arial, sans-serif" font-size="12" fill="#64748b">10FLOW Mobile</text>
  
  <!-- Header -->
  <rect x="20" y="60" width="260" height="50" fill="#3b82f6"/>
  <text x="30" y="85" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white">機能 2</text>
  
  <!-- Content area -->
  <rect x="30" y="130" width="240" height="80" fill="#f8fafc" rx="8"/>
  <rect x="40" y="140" width="220" height="20" fill="#e2e8f0" rx="4"/>
  <rect x="40" y="170" width="180" height="20" fill="#e2e8f0" rx="4"/>
  
  <rect x="30" y="230" width="240" height="80" fill="#f8fafc" rx="8"/>
  <rect x="40" y="240" width="220" height="20" fill="#e2e8f0" rx="4"/>
  <rect x="40" y="270" width="160" height="20" fill="#e2e8f0" rx="4"/>
  
  <!-- Chart/Graph area -->
  <rect x="30" y="330" width="240" height="120" fill="#ffffff" stroke="#e2e8f0" stroke-width="1" rx="8"/>
  <text x="40" y="355" font-family="Arial, sans-serif" font-size="14" fill="#64748b">データ可視化</text>
  
  <!-- Simple chart -->
  <rect x="50" y="370" width="20" height="60" fill="#3b82f6" rx="2"/>
  <rect x="80" y="390" width="20" height="40" fill="#10b981" rx="2"/>
  <rect x="110" y="380" width="20" height="50" fill="#f97316" rx="2"/>
  <rect x="140" y="370" width="20" height="60" fill="#8b5cf6" rx="2"/>
  
  <!-- Action button -->
  <rect x="50" y="480" width="200" height="40" fill="#f97316" rx="20"/>
  <text x="150" y="505" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">詳細を見る</text>
</svg>
